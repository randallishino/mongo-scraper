<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
    </div>
    <ul class="nav navbar-nav">
      <li id="home" class="active"><a href="/">Home</a></li>
      <li><a id="saved" href="/saved">Saved Articles</a></li>
    </ul>
  </div>
</nav>
<div class="jumbotron text-center">
  <div class="overlay">
  </div>
  <div class="background-image">
  </div>
  <div class="caption">
    <h1>New York Times Scraper!</h1>
  </div>
</div>
<div class="container article-container">
  {{#each article}}
  <div class="row">
    <div class="panel panel-default">
      <div class="panel-heading" id="panel-head">
        <h3 class="panel-title">{{this.title}}</h3>
      </div>
      <div class="panel-body">
        <a id="savedLink" href="{{this.link}}">{{this.link}}</a>
        <form class="navbar-form navbar-right" method="POST" action="/saved/{{this.id}}?_method=PUT">
          <button type="submit" class="btn btn-primary" id="save-articles">Save Article</button>
        </form>
      </div>
    </div>
  </div>
  {{/each}}
</div>
</footer>